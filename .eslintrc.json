{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser":"@babel/eslint-parser",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "classes": true
    }
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "18.0.0"
    }
  }
}